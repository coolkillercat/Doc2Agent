CODE:
# Define the project ID and the file path
project_id = new_repo['id']
file_path = "README.md"

# Create the content for the README file
readme_content = "# TODO Repository\n\n## Top 10 Active DIY Ideas\n\n"
for submission in top_10_active_submissions:
    readme_content += f"- [{submission['title']}]({submission['url']})\n"

# Update the README file in the repository
response = requests.put(
    f"{gitlab_base_url}/api/v4/projects/{project_id}/repository/files/{file_path}",
    headers={"PRIVATE-TOKEN": access_token},
    json={
        "branch": "main",
        "content": readme_content,
        "commit_message": "Add top 10 active DIY ideas to README"
    }
)
response.raise_for_status()  # Ensure we notice bad responses
updated_file = response.json()
updated_file
